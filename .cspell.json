{
    "useGitignore": true,
    "cache": {
        "useCache": true,
        "cacheLocation": "node_modules/.cache/cspell/.cspellcache",
        "cacheStrategy": "content"
    },
    "ignorePaths": ["CHANGELOG.md"],
    "ignoreWords": [
        "rimac",
        "packagerc",
        "loglevel",
        "pinst",
        "commitlint",
        "conventionalcommits",
        "postpack",
        "ENOACCESSKEYID",
        "ENOSECRETACCESSKEY",
        "ENOREGION",
        "ENOIMAGENAME",
        "ENOIMAGE",
        "EDEPLOY",
        "ENOAUTHENTICATION",
        "ENOAUTHORIZATION",
        "tsup"
    ]
}
